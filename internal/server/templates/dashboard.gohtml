{{ define "content" }}
<div class="border-dashed rounded-lg">
    <div class="grid gap-4 mb-4">
        {{ range .StartedSupervisors }}
            {{ template "supervisorCardStarted" . }}
        {{end}}
    </div>
    <h4 class="text-xl mb-2 font-extrabold dark:text-white">Non running</h4>
    <div class="grid grid-cols-2 gap-4">
        {{ range .StoppedSupervisors }}
            {{ template "supervisorCardStopped" . }}
        {{end}}
    </div>
</div>
{{ end }}